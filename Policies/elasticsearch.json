{
    "Statement": [
        {
            "Action": [
                "elasticsearch:Delete*",
                "elasticsearch:Update*",
                "elasticsearch:DeactivateZones"
            ],
            "Resource": [
                "*"
            ],
            "Effect": "Deny",
            "Condition": {
                "StringNotLike": {
                    "acs:PrincipalARN": "acs:ram:*:*:role/resourcedirectoryaccountaccessrole"
                }
            }
        }
    ],
    "Version": "1"
}